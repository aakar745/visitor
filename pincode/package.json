{
  "name": "india-pincode-fetcher",
  "version": "1.0.0",
  "description": "Standalone script to fetch India PIN code data from Postal API",
  "main": "fetch-postal-data.js",
  "scripts": {
    "fetch": "node fetch-postal-data.js",
    "fetch:resume": "node fetch-postal-data.js --resume",
    "fetch:range": "node fetch-postal-data.js --start=110001 --end=120000",
    "process": "node process-data.js",
    "export": "node export-to-excel.js",
    "import": "node import-to-db.js",
    "full-sync": "node fetch-postal-data.js && node process-data.js && node import-to-db.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "p-limit": "^3.1.0",
    "cli-progress": "^3.12.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "mongodb": "^6.3.0"
  },
  "keywords": [
    "india",
    "pincode",
    "postal",
    "api"
  ],
  "author": "",
  "license": "MIT"
}

